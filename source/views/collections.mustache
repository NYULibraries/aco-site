<!doctype html>
<html>
<head>{{> head }}</head>
<body data-app="{{ appUrl }}" data-appRoot="{{ appRoot }}" class="{{bodyClass}}">
  {{> header }}
  <div class="main">
  <h2>{{ title }}</h2>
  <span><a href="{{ appUrl }}" title="{{ appName }}">{{ appName }}</a> &gt; {{ title }}</span>
 </div>
  {{> footer }}
</body>
</html>