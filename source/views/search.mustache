<!doctype html>
<html>
<head>{{> head }}</head>
<body data-app="{{ appUrl }}" data-appRoot="{{ appRoot }}" class="{{bodyClass}}">
	{{> header }}
	<div class="main container-fluid">
		<div class="breadcrumbs" lang="en"><a href="{{ appUrl }}" title="{{ appName }}">{{ appName }}</a> &gt; {{ title }}</div>
		<h2>{{ title }}</h2>
		{{> search_form }}
		{{> items }}
	</div>
	{{> footer }}
</body>
</html>